print(" COMPARISON OPERATORS ")
print(" ")
print("Equals to ")
x=5
y=7
result = x == y
print(result)
print(" Not equals to ")
a = 10
b = 15
result = a != b
print(result)
print(" Greater than  ")
m = 2
n = 15
result = m > n
print(result)
print(" Less than  ")
p = 8
q = 12
result = p < q
print(result)
print(" Greater than or equals to   ")
hp = 10
dp = 10
result = hp >= dp
print(result)
