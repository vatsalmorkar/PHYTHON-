a=9
b=12
print("Addition:",a+b)
print("Substraction:",a-b)
print("Multiplication:",a*b) 
print("Division:",a/b)
print("Floor Division:",a//b)
print("Modulas Operator:",a%b)
print("Exponentiation Operator:",a**b) 
print("Now,   a=10")
print("       b=5")       
a=10
b=5
a+=3
print("addition assignment(+=):",a)
a-=2
print("Substraction assignment(-=):",a)
a *=4
print("Multiplication assignment(*=):",a)
a /=2
print("Division assignment(/=):",a)
a //= 2
print("Floor Division Assignment (//=):", a)
a %= 3
print("Modulus Assignment (%=):", a)
a **= 2
print("Exponentiation Assignment (**=):", a)

